{
	"expo": {
		"name": "Concurro",
		"slug": "Concurro",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"backgroundColor": "#1D2025",
					"image": "./assets/images/icon.png"
				}
			],
			"expo-sqlite"
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
